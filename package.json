{
  "name": "@intugine-technologies/radiant",
  "version": "2.0.0-alpha",
  "type": "module",
  "description": "Radiant design library",
  "repository": "http://github.com/navneetlal/radiant-design-system",
  "author": "Navneet Lal Gupta <navneetlalg@gmail.com>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "clean": "rimraf build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --cache",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mui/base": "^5.0.0-alpha.80",
    "@mui/system": "^5.7.0",
    "@mui/types": "^7.1.3",
    "@mui/utils": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
